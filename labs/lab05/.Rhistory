target <- function(x, a, b){ifelse((x >= 0) & (x <= 1), (dexp(x, rate=5)/0.9932621) * dbinom(a,b,prob = x), 0)}
result <- runMetropolis(iterations = 500000, initialValue=0.5, numSuccess=14, totalNum=24)
myHist <- hist(result, breaks = 200, plot=FALSE)
plot(myHist$mids, myHist$counts/length(result))
expSum <- sum(target(myHist$mids, numSuccess, totalNum))
lines(myHist$mids, target(myHist$mids, numSuccess, totalNum)/expSum, col="red")
